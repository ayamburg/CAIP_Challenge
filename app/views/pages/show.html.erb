<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="https://d3js.org/d3.v5.min.js"></script>

<div class="page">
  <div class="page__header">
    <h1 id="search-term"></h1>
  </div>

  <div class="tabs">
    <div class="tab-menu">
      <div class="tab-menu__item selected" target="#page1">Accordion</div>
      <div class="tab-menu__item" target="#page2">Graph</div>
    </div>
  
    <div class="tab-content " id="page1">
      <div class="card no-pad box-shadow">
        <div class="search-bar">
          <div class="searchContainer">
            <i class="fa fa-search searchIcon"></i>
            <input title="search" placeholder="Text">
          </div>
          <div class="buttonContainer">
            <button class="button reset">Reset</button>
            <button class="button primary search">Search</button>
          </div>
        </div>
        <div id="videos-accordion"></div>
      </div>
    </div>
  
    <div class="tab-content hidden" id="page2">
      <div class="card box-shadow">
        <div class="bar-graph">
        </div>
      </div>
    </div>
  </div>
</div>
